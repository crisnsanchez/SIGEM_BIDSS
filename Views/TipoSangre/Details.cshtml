@model SIGEM_BIDSS.Models.TbTipoSangre

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="card shadow mb-4">
    <!-- Card Header - Dropdown -->
    <div class="card-header py-3 d-flex  align-items-center justify-content-between">
        <h6 class="m-0 font-weight-bold text-primary">Detalle Tipo Sangre</h6>
        <div class="float-right">
        </div>
    </div>

    <div class="card-body">

        <div class="container-fluid">
            <div class="col-md-4">
                <dl class="dl-horizontal">
                    <dt class="col-sm-10">
                        ID
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.TpsId)
                    </dd>
                    <dt class="col-sm-10">
                        Tipo de Nombre
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.TpsNombre)
                    </dd>



                </dl>

                <div>
                    <a asp-action="Edit" class="btn btn-primary" asp-route-id="@Model.TpsId">Editar</a>
                    <a asp-action="Index" class="btn btn-danger">Regresar al Listado</a>
                </div>
            </div>
        </div>
    </div>
    </div>
