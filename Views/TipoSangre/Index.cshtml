@model IEnumerable<SIGEM_BIDSS.Models.TbTipoSangre>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Tipos De Sangre</h1>

<p>
    <a asp-action="Create">Crear Nuevo</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Nombre
            </th>
            <th>
                Usuario Crea
            </th>
            <th>
                Fecha Crea
            </th>
            <th>
                Usuario Modifica
            </th>
            <th>
                Fecha Modifica
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TpsNombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TpsUsuarioCrea)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TpsFechaCrea)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TpsUsuarioModifica)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TpsFechaModifica)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.TpsId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.TpsId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.TpsId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
