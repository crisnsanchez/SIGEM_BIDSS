@model SIGEM_BIDSS.Models.TbDepartamento

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Create</h1>

<h4>TbDepartamento</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="DepId" class="control-label"></label>
                <input asp-for="DepId" class="form-control" />
                <span asp-validation-for="DepId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DepDescripcion" class="control-label"></label>
                <input asp-for="DepDescripcion" class="form-control" />
                <span asp-validation-for="DepDescripcion" class="text-danger"></span>
            </div>
   
            <div class="form-group">
                <input type="submit" value="Guardar" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-danger">Cancelar</a>
            </div>
        </form>
    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
