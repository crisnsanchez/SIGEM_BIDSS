@model IEnumerable<SIGEM_BIDSS.Models.TbTipoSolicitud>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Tipos De Solicitud</h1>

<p>
    <a asp-action="Create">Crear Nuevo</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Descripción
            </th>
            <th>
                Usuario Crea
            </th>
            <th>
                Fecha Crea
            </th>
            <th>
                Usuario Modifica
            </th>
            <th>
                Fecha Modifica
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TipsolDescripcion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipsolUsuarioCrea)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipsolFechaCrea)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipsolUsuarioModifica)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipsolFechaModifica)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.TipsolId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.TipsolId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.TipsolId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
