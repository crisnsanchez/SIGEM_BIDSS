@model IEnumerable<SIGEM_BIDSS.Models.TbTipoMoneda>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Listado De Tipos De Monedas</h1>

<p>
    <a asp-action="Create">Crear Nuevo</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Abreviatura
            </th>
            <th>
                Nombre
            </th>
            <th>
                Usuario Crea
            </th>
            <th>
                Fecha Crea
            </th>
            <th>
                Usuario Modifica
            </th>
            <th>
                Fecha Modifica
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TmoAbreviatura)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TmoNombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TmoUsuarioCrea)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TmoFechaCrea)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TmoUsuarioModifica)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TmoFechaModifica)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.TmoId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.TmoId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.TmoId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
