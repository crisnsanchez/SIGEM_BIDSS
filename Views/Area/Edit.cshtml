@model SIGEM_BIDSS.Models.TbArea

@{
    ViewData["Title"] = "Edit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Editar Area</h2>


<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="AreId" />
            <div class="form-group">
                <label asp-for="AreId" class="control-label"></label>
                <input asp-for="AreId" class="form-control" readonly />
                <span asp-validation-for="AreId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AreDescripcion" class="control-label"></label>
                <input asp-for="AreDescripcion" class="form-control" />
                <span asp-validation-for="AreDescripcion" class="text-danger"></span>
            </div>

            <div class="form-group">

                <input type="submit" value="Guardar" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-danger">Regresar al Listado</a>
            </div>
        </form>
    </div>
</div>

<div>
 
</div>

