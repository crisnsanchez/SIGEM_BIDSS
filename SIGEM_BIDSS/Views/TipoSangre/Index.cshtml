@model IEnumerable<SIGEM_BIDSS.Models.tbTipoSangre>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-striped table-responsive " style="width:100%" id="dataTable">
    <thead>
        <tr>

            <th>
                @Html.DisplayNameFor(model => model.tps_nombre)
            </th>
     
            <th>ACCIONES</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.tps_nombre)
                </td>
           
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.tps_Id }) |
                    @Html.ActionLink("Details", "Details", new { id = item.tps_Id }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.tps_Id })
                </td>
            </tr>
        }

    </tbody>
</table>