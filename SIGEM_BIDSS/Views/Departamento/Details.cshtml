@model SIGEM_BIDSS.Models.tbDepartamento

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col-sm-4">
        <h2>Departamento</h2>
    </div>
</div>

<div class="ibox float-e-margins">
    <div class="ibox-title">
        <h5>Detalles Departamento</h5>
    </div>
    <div class="ibox-content">
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.dep_Codigo)
            </dt>
            <dd>
                @Html.DisplayFor(model => model.dep_Codigo)
            </dd>
            <dt>
                @Html.DisplayNameFor(model => model.dep_Nombre)
            </dt>
            <dd>
                @Html.DisplayFor(model => model.dep_Nombre)
            </dd>
        </dl>
        <div class="col-md-12">
            @Html.ActionLink("Editar", "Edit", new { id = Model.dep_Codigo }, new { @class = "btn btn-primary" })
        </div>
        <div class="row">
            <div class="col-lg-12">
                <div class="form-group">
                    <div class="col-md-12">
                        @Html.Partial("_DetailsMunicipio", Model.tbMunicipio)
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.dep_Codigo }) |
    @Html.ActionLink("Regresar a la lista", "Index")
</p>

<script src="~/Scripts/jquery-3.3.1.min.js"></script>
<script src="~/Scripts/app/Departamento.js"></script>