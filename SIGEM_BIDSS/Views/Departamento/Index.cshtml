@model IEnumerable<SIGEM_BIDSS.Models.tbDepartamento>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Crear", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.dep_Codigo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.dep_Nombre)
        </th>
        
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.dep_Codigo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dep_Nombre)
            </td>
            
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.dep_Codigo }) |
                @Html.ActionLink("Details", "Details", new { id = item.dep_Codigo }) |
            </td>
        </tr>
    }
</table>
<script src="~/Scripts/jquery-3.3.1.min.js"></script>
